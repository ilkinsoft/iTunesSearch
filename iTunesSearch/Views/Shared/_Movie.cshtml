@model IEnumerable<iTunesSearch.DTO.Movie>


@foreach (var movie in Model)
{
    <div class="row">
        <div class="col-md-3">
            @movie.artistName <br />
            @movie.primaryGenreName <br />
            @movie.trackName <br />
            @movie.country <br />
            @movie.trackViewUrl <br />

        </div>
        <div class="col-md-3">
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
        </div>
    </div>
}

